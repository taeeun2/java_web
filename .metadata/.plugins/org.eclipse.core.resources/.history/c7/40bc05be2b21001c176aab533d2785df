function searchPost(){
	var dong = frmMember.post.value.trim();
	console.log(dong.length+'->'+dong);
	if(dong.length == null || dong == '')
		alert("~동을 입력해주세요");
	else{
		window.open('/webapp/PostServlet?dong='+dong,'pop','width=600,height=500');
	}

}