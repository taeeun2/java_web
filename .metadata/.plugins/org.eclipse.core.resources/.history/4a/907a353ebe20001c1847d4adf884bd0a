<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
    <%@ taglib uri = "http://java.sun.com/jsp/jstl/core" prefix = "c" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>JSTL</title>
</head>
<body>
<h2>c:forEach 태그</h2>
<h3>반복문 - 배열</h3>
<% pageContext.setAttribute("nameList", new String[]{"홍길동","임꺽정","일지매"}); %>
<ul>
	<c:forEach var = "name" : items =  "${nameList}">
		<li>${name}</li>
	</c:forEach>
</ul>
</body>
</html>